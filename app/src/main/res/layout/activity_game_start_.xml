<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.user.duty.game_start_Activity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1.7">

        <TextView
            android:id="@+id/count_view"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="count"
            android:textSize="30dp" />

        <TextView
            android:id="@+id/time_view"
            android:gravity="center"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="30dp"
            android:text="time"/>

    </LinearLayout>

    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@drawable/backcroko">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            >

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="15dp">

                <ImageView
                    android:id="@+id/teeth1_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleY="1.2"
                    android:scaleX="1.3"
                    android:src="@drawable/vkfk" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_marginLeft="17dp"
                android:layout_width="70dp"
                android:layout_height="match_parent">



                <ImageView
                    android:id="@+id/teeth2_view"

                    android:scaleX="1.2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/vkfk" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_marginLeft="15dp"
                android:layout_width="70dp"
                android:layout_height="match_parent">


                <ImageView
                    android:id="@+id/teeth3_view"
                    android:scaleX="1.2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/vkfk" />


            </LinearLayout>

            <LinearLayout

                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                android:layout_width="70dp"
                android:layout_height="match_parent">


                <ImageView
                    android:id="@+id/teeth4_view"

                    android:scaleX="1.2"
                    android:scaleY="1.2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/vkfk" />

            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp">

            <LinearLayout
                android:layout_marginLeft="9dp"
                android:layout_width="70dp"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/teeth5_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/vkfk" />

            </LinearLayout>


            <LinearLayout
                android:layout_marginLeft="250dp"
                android:layout_width="70dp"
                android:layout_height="match_parent">

                <ImageView
                    android:scaleX="1.1"
                    android:id="@+id/teeth6_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/vkfk" />


            </LinearLayout>





        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp">


            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"

                android:layout_marginLeft="12dp">

                <ImageView
                    android:id="@+id/teeth7_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/vkfk" />

            </LinearLayout>



            <LinearLayout
                android:layout_marginLeft="244dp"
                android:layout_width="70dp"
                android:layout_height="match_parent">


                <ImageView
                    android:id="@+id/teeth8_view"

                    android:scaleX="1.1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/vkfk" />



            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp">


            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"

                android:layout_marginLeft="16dp">


                <ImageView
                    android:id="@+id/teeth9_view"

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/vkfk" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"

                android:layout_marginLeft="238dp">


                <ImageView
                    android:id="@+id/teeth10_view"

                    android:layout_width="match_parent"
                    android:scaleX="1.1"
                    android:layout_height="match_parent"
                    android:src="@drawable/vkfk" />

            </LinearLayout>




        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical"
        android:background="@drawable/backcroko1">


        <LinearLayout
            android:layout_marginTop="12dp"
            android:layout_width="match_parent"
            android:layout_height="70dp">

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="23dp">

                <ImageView
                    android:id="@+id/teeth11_view"

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/teetetee" />



            </LinearLayout>



            <LinearLayout
                android:layout_width="70dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="230dp"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/teeth12_view"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/teetetee" />

            </LinearLayout>



        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp">

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="17dp">



                <ImageView
                    android:id="@+id/teeth13_view"

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/teetetee" />
            </LinearLayout>



            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="240dp"
                android:layout_marginTop="5dp">

                <ImageView
                    android:id="@+id/teeth14_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"

                    android:src="@drawable/teetetee" />

            </LinearLayout>



        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp">


                <ImageView
                    android:id="@+id/teeth15_view"
                    android:scaleType="fitXY"
                    android:scaleX="1.2"
                    android:scaleY="1.2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/teetetee" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="250dp"
                android:layout_width="70dp"
                android:layout_height="match_parent">



                <ImageView
                    android:id="@+id/teeth16_view"

                    android:scaleX="1.3"
                    android:scaleY="1.2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/teetetee" />

            </LinearLayout>



        </LinearLayout>


        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="60dp">

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="45dp">


                <ImageView
                    android:id="@+id/teeth17_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleX="1.2"
                    android:scaleY="1.2"

                    android:src="@drawable/teetetee" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp">


                <ImageView
                    android:id="@+id/teeth18_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleX="1.3"
                    android:scaleY="1.1"
                    android:src="@drawable/teetetee" />

            </LinearLayout>


            <LinearLayout

                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="18dp">



                <ImageView
                    android:id="@+id/teeth19_view"


                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleX="1.3"
                    android:scaleY="1.1"
                    android:src="@drawable/teetetee" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="15dp">

                <ImageView
                    android:id="@+id/teeth20_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleY="1.2"
                    android:scaleX="1.2"


                    android:src="@drawable/teetetee" />
            </LinearLayout>




        </LinearLayout>

    </LinearLayout>


</LinearLayout>
